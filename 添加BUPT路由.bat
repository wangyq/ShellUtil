::添加BUPT校园网路由
@ECHO OFF
COLOR 0A

SET BUPT_GATEWAY=10.108.100.1
SET BUPT_NET=10.0.0.0

ECHO "添加BUPT校园网路由, Gateway=%BUPT_GATEWAY%"

::ROUTE -p ADD 10.0.0.0 MASK 255.0.0.0 %BUPT_GATEWAY%
::ROUTE -p ADD %BUPT_NET% MASK 255.0.0.0 %BUPT_GATEWAY%
ROUTE ADD %BUPT_NET% MASK 255.0.0.0 %BUPT_GATEWAY%


ROUTE PRINT

ECHO . & PAUSE
